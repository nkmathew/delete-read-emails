{
  "env": {
    "browser": true
  },
  "globals": {
    "Component": 1,
    "MailUtils": 1,
    "MsgMoveMessage": 1,
    "gFolderDisplay": 1,
    "gDBView": 1,
    "goDoCommand": 1,
    "Components": 1,
    "fixIterator": 1
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "rules": {
    "brace-style": [2, "1tbs"],
    "consistent-return": 2,
    "curly": [2, "multi-line"],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": [2, "always"],
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-floating-decimal": 2,
    "no-param-reassign": 2,
    "no-self-compare": 2,
    "no-shadow": 0,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 0,
    "no-var": 2,
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-infix-ops": 2,
    "keyword-spacing": [2, {"before": true, "after": true}],
    "spaced-comment": 2,
    "strict": 0,
    "vars-on-top": 2,
    "wrap-iife": 2
  }
}
